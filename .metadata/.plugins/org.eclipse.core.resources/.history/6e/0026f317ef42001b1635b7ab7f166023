public	class cash3random {

			public static void main(String[] args) {
				Scanner mcash3 = new Scanner(System.in);  // Create a Scanner object
			    System.out.println("Enter midday cash3");
			    
			    Scanner ecash3 = new Scanner(System.in);  // Create a Scanner object
			    System.out.println("Enter evening cash3");
			    
			    Scanner ncash3 = new Scanner(System.in);  // Create a Scanner object
			    System.out.println("Enter evening cash3");
				
			    String lastNumbers = mcash3.nextLine();  // Read user input
			    System.out.println("Numbers that fell: " + lastNumbers);  // Output user input
			    
			    String eveningNumbers = ecash3.nextLine();  // Read user input
			    System.out.println("Numbers that fell: " + eveningNumbers);  // Output user input
			    
			    String nightNumbers = ecash3.nextLine();  // Read user input
			    System.out.println("Numbers that fell: " + nightNumbers);


			Random cash3 = new Random();
		   System.out.println(	
			cash3.nextInt(10) + " " + 
			cash3.nextInt(10) + " "+ 
			cash3.nextInt(10));

		}

	}
